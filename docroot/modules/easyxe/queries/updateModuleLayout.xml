<query id="updateModuleLayout" action="update">
    <tables>
        <table name="modules" />
    </tables>
    <columns>
        <column name="layout_srl" var="target_layout_srl" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="layout_srl" var="source_layout_srl" notnull="notnull" />
    </conditions>
</query>
