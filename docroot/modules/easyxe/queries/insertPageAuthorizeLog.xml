<query id="insertPageAuthorizeLog" action="insert">
	<tables>
		<table name="easyxe_page_auth_log" />
	</tables>
	<columns>
		<column name="auth_srl" var="auth_srl" default="sequence()" notnull="notnull" filter="number" />
		<column name="module_srl" var="module_srl" notnull="notnull" filter="number" />
		<column name="member_srl" var="member_srl" default="0" notnull="notnull" filter="number" />
		<column name="ipaddress" var="ipaddress" default="ipaddress()" notnull="notnull" />
		<column name="point" var="point" default="0" notnull="notnull" />
		<column name="date" var="date" notnull="notnull" default="curdate()" />
	</columns>
</query>