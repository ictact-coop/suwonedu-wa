<query id="updateRssboard" action="update">
	<tables>
		<table name="rssboard"/>
	</tables>
	<columns> 
		<column name="module_srl" var="module_srl" default="0" notnull="notnull"/>
		<column name="rssurl" var="rssurl" notnull="notnull" />
		<column name="rssname" var="rssname" notnull="notnull" />
		<column name="category_srl" var="category_srl" default="0"/>
	</columns>
	<conditions>
		<condition operation="equal" column="rssboard_srl" var="rssboard_srl" filter="number" notnull="notnull"/>
	</conditions>
</query>